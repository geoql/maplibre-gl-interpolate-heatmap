{
  "$schema": "./node_modules/oxfmt/configuration_schema.json",
  "printWidth": 80,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "ignorePatterns": [
    "dist",
    "node_modules",
    "coverage",
    "bun.lock",
    "*.lock",
    "CHANGELOG.md",
    "jsr.json",
  ],
}
